// exercise 9
var x=50;
var order='Beer';
if (x>50){
    document.write("He's over 50!");
}else if (x <= 50 && x>30){
document.write("Middle aged man, wgho ordered some "+order);
} else {
document.write("It seems you're bit underaged.");
}

